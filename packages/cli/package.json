{
  "name": "maestro-cli",
  "version": "0.1.0",
  "description": "CLI for Maestro - orchestrate AI coding agents",
  "type": "module",
  "bin": {
    "maestro": "./dist/cli.js"
  },
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup",
    "clean": "rm -rf dist",
    "dev": "tsup --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@maestro/sdk": "*",
    "chalk": "^5.3.0",
    "cli-table3": "^0.6.5",
    "commander": "^12.1.0",
    "ora": "^8.1.1",
    "yaml": "^2.6.1"
  },
  "devDependencies": {
    "@types/node": "^25.0.0",
    "tsup": "^8.3.5",
    "typescript": "~5.5.4"
  },
  "engines": {
    "node": ">=22.0.0"
  }
}
